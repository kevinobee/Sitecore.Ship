<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MajorVersion>0</MajorVersion>
    <MinorVersion>4</MinorVersion>
    <PatchVersion>0</PatchVersion>

   
  <!-- Change PRERELEASE to an empty value or comment out the tag to prevent package names being generated with a -->
  <!-- build quality and YYMMDD suffix, eg. Package.Name.0.1.0.130207.nupkg -->
    <PRERELEASE>Alpha 1</PRERELEASE>
    
    <!-- Ensure that we come up with a new version every 65535 years -->
    <VersionStartYear>2013</VersionStartYear>
  </PropertyGroup>
</Project>