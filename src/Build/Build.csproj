<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <AssemblyTitle>Build</AssemblyTitle>
    <Product>Build</Product>
    <Copyright>Copyright ©  2016</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MSBuild.Microsoft.VisualStudio.Web.targets" Version="12.0.4" />
    <PackageReference Include="NuGet.CommandLine" Version="2.8.6" />
    <PackageReference Include="ReportGenerator" Version="2.4.5.0" />
    <PackageReference Include="xunit.runner.msbuild" Version="2.1.0" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.1.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\Common\GlobalSuppressions.cs" Link="Properties\GlobalSuppressions.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\build.cmd">
      <Link>command-line\build.cmd</Link>
    </None>
    <None Include="..\..\build\Build.proj">
      <Link>command-line\Build.proj</Link>
      <SubType>Designer</SubType>
    </None>
    <None Include="..\..\build\build.props">
      <Link>command-line\build.props</Link>
    </None>
    <None Include="..\..\build\build.targets">
      <Link>command-line\build.targets</Link>
      <SubType>Designer</SubType>
    </None>
    <None Include="..\..\build\Build.tasks">
      <Link>command-line\Build.tasks</Link>
    </None>
    <None Include="..\..\build\environment.props">
      <Link>command-line\environment.props</Link>
    </None>
    <None Include="..\..\build\README.md">
      <Link>README.md</Link>
    </None>
    <None Include="..\..\build\smokeTests.targets">
      <Link>command-line\smokeTests.targets</Link>
    </None>
  </ItemGroup>
